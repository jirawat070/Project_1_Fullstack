<% include ../partials/header %>

<form action="/user/insert" method="post">
    <div class="form-group">
        <label for="id">ID</label>
        <input type="number" class="form-control" name="id" id="" aria-describedby="helpId" placeholder="" value="">
        <small id="helpId" class="form-text text-muted"> ID</small>
    </div>
    <div class="form-group">
        <label for="email"></label>
        <input type="text" class="form-control" name="email" id="" aria-describedby="helpId" placeholder="" value="">
        <small id="helpId" class="form-text text-muted">Email</small>
    </div>
    
    <div class="form-group">
         <label for="password">Password</label>
         <input type="password" class="form-control" name="password" id="" placeholder="">
    </div>

    <div class="form-group">
        <input type="text" class="form-control" value="<%= time %>" name="time" id="" aria-describedby="helpId"
            placeholder="">
        <small id="helpId" class="form-text text-muted">Time</small>
    </div>



    <button type="submit" class="btn btn-success">Save</button>

</form>
<% include ../partials/footer %>